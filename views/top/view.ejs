<%- include('../header.ejs') %>
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    
<div class="container-fluid" >
    <div class="row m-2">
       <div class="col-md-1"></div>
      <div class="col-md-10">
        <br><br><br><br><br>
        <h3 class="alert alert-dark text-center">DATA VIEW</h3>
        
        <table class="table  table-hover table-responsive table-dark ">
            <center>
                <thead>
                    <tr>
                     
                      <th scope="col">No.</th>
                      
                      <th scope="col">id</th>
                      <th scope="col">Username</th>
                      <th scope="col">Email</th>
                      <th scope="col">Mobile Number</th>
                      <th scope="col">Gender</th>
                      <th scope="col">College</th>
                      
                      <th scope="col">Course</th>
                      <th scope="col">Branch</th>
                     
                    </tr>
                   
                  </thead>
                  <tbody>
                       
                        
                            <tr>
                                <td>1</td>
                                <td><%= data._id %></td>
                                <td><%= data.username %></td>
                                <td><%= data.email %></td>
                                <td><%= data.mobile %></td>
                                <td><%= data.address %></td>
                                <td><%= data.gender %></td>
                                <td><%= data.college %></td>
                                <td><%= data.course %></td>
                                <td><%= data.branch %></td>
                                
                              </tr>

                             
                              

                            </tbody>
                        
                       
                        </center>
                      </table>
                      <center> <br>
                        <a href="/displaybtech" class="btn btn-warning mb-3">Back To The Display</a>
                      </center>
                     
                    
                       
            </div>
            <div class="col-md-1"></div>
        </div>
    </div>
